<?xml version="1.0" encoding="utf-8"?>
<!--Trying to get inner white rectangle surrounding blue color, Daniel-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/seekbar_white_progress">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white"/>
            <corners android:radius="10dp"/>
            <size android:height="50dp"
                android:width="100dp"/>
        </shape>
    </item>
    <item android:id="@+id/seekbar_blue_progress"
        android:right="5dp" android:left="5dp"
        android:top="5dp" android:bottom="5dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/blue"/>
            <corners android:radius="10dp"/>
            <size android:height="40dp"/>
        </shape>
    </item>
</layer-list>